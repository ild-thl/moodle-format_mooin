
<div class="course-title-header">
  {{{edit_header_link}}}
  {{#course_headerimage_mobil}}
  <img src="{{course_headerimage_mobil}}" alt="" class="course-image d-block d-sm-none">
  {{/course_headerimage_mobil}}
  {{^course_headerimage_mobil}}
  <div class="course-image course-image-none-mobile d-block d-sm-none"></div>
  {{/course_headerimage_mobil}}
  {{#course_headerimage_desktop}}
  <img src="{{course_headerimage_desktop}}" alt="" class="course-image d-none d-sm-block">
  {{/course_headerimage_desktop}}
  {{^course_headerimage_desktop}}
  <div class="course-image course-image-none-desktop d-none d-sm-block"></div>
  {{/course_headerimage_desktop}}

  <div class="title-overlay fade-out">
    <p>{{#str}} welcome, format_mooin4 {{/str}}</p>
    <h2>{{coursename}}</h2>
    <a href="{{{continue_url}}}" id="continue-mobile" class="mooin4-btn mooin4-btn-primary btn-continue d-flex d-sm-none">{{continue_text}}</a>
  </div>
</div>

{{#js}}
require(['theme_mooin4/title-overlay'], function(module) {
module.fadeOutTitleOverlay();
})
{{/js}}